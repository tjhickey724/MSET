extends layout

block content
  h1= title
  p This is a page for debugging in the console
  | Press CMD-ALT-J on Mac  <br> or F-12 on windows<br>
  | to open the Javascript console in Chrome <br>
  | (and if you aren't using Chrome, then switch!!)
  br
  select#test
      option('selected') numEdits
      option numClients
      option initialSize
      option burstSize
      option p2p
  |
  | k=
  input#kTF(type="text",value="10")
  |
  | multiplier=
  input#multTF(type="text", value="1")
  button#go RUN TESTS
  hr
  | numEdits=
  input#numEditsTF(type="text",value="10")
  br
  | numClients=
  input#numListsTF(type="text",value="2")
  br
  |initSize=
  input#initSizeTF(type="text",value="10")
  br
  |burstSize=
  input#burstSizeTF(type="text",value="1")
  br
  |shuffle?=
  input#shuffleCB(type="checkbox",value="1")
  br
  hr

  #results

  script(src="https://code.jquery.com/jquery-1.11.1.js")
  script(src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.1.0/socket.io.js")

  // script(type="module" src="/testing/testingDLLmset.js")
  // script(type="module" src="/testing/testDLLwiCorrectness.js")
  script(type="module" src="/testing/TestSocket.js")
  script(type="module" src="/testing/testingDDLLmset.js")
